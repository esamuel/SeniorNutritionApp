"health_tip_diabetes_monitor_description" = "בדקו את רמת הסוכר בדם לפי המלצות איש המקצוע. ניטור קבוע מסייע להבין כיצד התזונה, הפעילות והתרופות משפיעים על הרמות שלכם.";

"health_tip_diabetes_glycemic_title" = "בחרו מזונות עם אינדקס גליקמי נמוך";
"health_tip_diabetes_glycemic_description" = "מזונות עם אינדקס גליקמי נמוך מעלים את רמת הסוכר בדם לאט יותר. העדיפו דגנים מלאים, קטניות וירקות לא עמילניים על פני פחמימות מעובדות.";

"health_tip_diabetes_meal_timing_title" = "אכלו את הארוחות שלכם בשעות קבועות";
"health_tip_diabetes_meal_timing_description" = "נסו לאכול באותה שעה בכל יום כדי לשמור על רמת סוכר יציבה בדם. הימנעו מלדלג על ארוחות, במיוחד אם אתם נוטלים תרופות.";

"health_tip_diabetes_active_title" = "הישארו פעילים לאחר הארוחות";
"health_tip_diabetes_active_description" = "הליכה קצרה לאחר ארוחות יכולה לעזור להפחית את עליות הסוכר לאחר הארוחה. שאפו להליכה של 10-15 דקות לאחר הארוחה העיקרית שלכם ביום.";

"health_tip_diabetes_stress_title" = "נהלו את רמות המתח שלכם";
"health_tip_diabetes_stress_description" = "מתח יכול להעלות את רמת הסוכר בדם. תרגלו טכניקות הרגעה כמו נשימה עמוקה, מדיטציה או יוגה עדינה כדי לעזור בניהול המתח.";

"health_tip_diabetes_feet_title" = "בדקו את הרגליים שלכם מדי יום";
"health_tip_diabetes_feet_description" = "אנשים עם סוכרת צריכים לבדוק את הרגליים שלהם מדי יום לחיתוכים, שלפוחיות, אדמומיות או נפיחות. טיפול טוב ברגליים מונע סיבוכים.";

"health_tip_diabetes_hydration_title" = "הישארו מיוהדרים";
"health_tip_diabetes_hydration_description" = "הידרציה טובה מסייעת לכליות שלכם לסלק עודף סוכר דרך השתן. שאפו ל-8 כוסות מים ביום, יותר אם רמת הסוכר שלכם גבוהה.";

"health_tip_diabetes_fiber_title" = "הכלילו סיבים בכל ארוחה";
"health_tip_diabetes_fiber_description" = "סיבים מאטים את ספיגת הסוכר ומשפרים את רמת הסוכר בדם. שאפו ל-25-30 גרם סיבים יומיים מירקות, פירות, דגנים מלאים וקטניות.";

"health_tip_diabetes_alcohol_title" = "הגבילו צריכת אלכוהול";
"health_tip_diabetes_alcohol_description" = "אלכוהול עלול להוריד את רמת הסוכר בדם באופן מסוכן אם אתם נוטלים אינסולין או תרופות מסוימות לסוכרת. אם אתם שותים, עשו זאת במתינות ותמיד עם אוכל.";

"health_tip_diabetes_abc_title" = "הכירו את ה-ABC שלכם";
"health_tip_diabetes_abc_description" = "לניהול סוכרת, זכרו את ה-ABC שלכם: A1C (בדיקת סוכר), לחץ דם (Blood pressure) וכולסטרול (Cholesterol). שמרו על שלושתם בטווחים המטרה.";

"health_tip_diabetes_portion_title" = "בקרת מנות חשובה";
"health_tip_diabetes_portion_description" = "אפילו מזונות בריאים יכולים להעלות את רמת הסוכר בדם אם המנות גדולות מדי. השתמשו בכוסות מדידה או משקל מזון עד שתוכלו להעריך במדויק גדלי מנות.";

"health_tip_diabetes_fats_title" = "בחרו שומנים בריאים";
"health_tip_diabetes_fats_description" = "החליפו שומנים רוויים בשומנים בריאים ממקורות כמו שמן זית, אבוקדו ואגוזים כדי לשפר את רגישות האינסולין ובריאות הלב.";

"health_tip_diabetes_low_sugar_title" = "היו מוכנים להיפוגליקמיה";
"health_tip_diabetes_low_sugar_description" = "החזיקו תמיד פחמימה מהירת פעולה כמו טבליות גלוקוז, מיץ או סוכריות קשות לטיפול בהיפוגליקמיה (רמת סוכר נמוכה בדם).";

"health_tip_diabetes_eye_title" = "תכננו בדיקות עיניים סדירות";
"health_tip_diabetes_eye_description" = "סוכרת מגדילה את הסיכון לבעיות עיניים. ערכו בדיקת עיניים מקיפה עם הרחבת אישונים לפחות פעם בשנה כדי לגלות בעיות מוקדם.";

"health_tip_diabetes_sleep_title" = "תנו עדיפות לשינה איכותית";
"health_tip_diabetes_sleep_description" = "שינה לקויה יכולה להשפיע על שליטה ברמת הסוכר בדם ורגישות לאינסולין. שאפו ל-7-8 שעות שינה איכותית כל לילה.";

/* Blood Pressure Tips */
"health_tip_bp_sodium_title" = "הפחיתו את צריכת הנתרן";
"health_tip_bp_sodium_description" = "הגבילו את הנתרן ל-1,500-2,300 מ\"ג ביום. קראו תוויות מזון, הימנעו ממזונות מעובדים והשתמשו בעשבי תיבול ותבלינים במקום מלח לתיבול.";

"health_tip_bp_dash_title" = "אמצו את דיאטת DASH";
"health_tip_bp_dash_description" = "דיאטת DASH (גישות תזונתיות לעצירת יתר לחץ דם) מדגישה פירות, ירקות, דגנים מלאים, חלבונים רזים ומוצרי חלב דלי שומן, תוך הגבלת נתרן ושומנים רוויים.";

"health_tip_bp_monitor_title" = "נטרו את לחץ הדם שלכם";
"health_tip_bp_monitor_description" = "ניטור קבוע בבית מסייע לכם לעקוב אחר התקדמותכם ומתריע לכם על בעיות אפשריות. נהלו יומן של המדידות שלכם כדי לשתף אותן עם הרופא שלכם.";

"health_tip_bp_alcohol_title" = "הגבילו צריכת אלכוהול";
"health_tip_bp_alcohol_description" = "אלכוהול מוגזם יכול להעלות את לחץ הדם. הגבילו עצמכם לכוסית אחת ביום לנשים ושתיים לגברים, או שקלו להימנע מאלכוהול לחלוטין.";

"health_tip_bp_potassium_title" = "הגבירו את צריכת האשלגן";
"health_tip_bp_potassium_description" = "אשלגן מסייע לאזן את הנתרן בתאים שלכם. כללו מזונות עשירים באשלגן כמו בננות, תפוחי אדמה, אבוקדו וירקות ירוקים בתזונה שלכם.";

"health_tip_bp_stress_title" = "תרגלו ניהול מתח";
"health_tip_bp_stress_description" = "מתח כרוני תורם ליתר לחץ דם. נסו נשימה עמוקה, מדיטציה, יוגה או טכניקות הרפיה אחרות באופן יומי.";

"health_tip_bp_weight_title" = "שמרו על משקל בריא";
"health_tip_bp_weight_description" = "אפילו ירידה צנועה במשקל יכולה להפחית משמעותית את לחץ הדם. שאפו למדד מסת גוף (BMI) בין 18.5 ל-24.9.";

"health_tip_bp_exercise_title" = "התעמלו באופן קבוע";
"health_tip_bp_exercise_description" = "שאפו ללפחות 150 דקות של פעילות מתונה בשבוע. פעילות גופנית סדירה מחזקת את הלב שלכם ומסייעת לו לשאוב ביעילות רבה יותר.";

"health_tip_bp_caffeine_title" = "הגבילו את צריכת הקפאין";
"health_tip_bp_caffeine_description" = "קפאין יכול להעלות את לחץ הדם באופן זמני. הגבילו קפה, תה ומשקאות אנרגיה, במיוחד אם יש לכם יתר לחץ דם.";

"health_tip_bp_medication_title" = "קחו את התרופות שלכם כמושחם";
"health_tip_bp_medication_description" = "תרופות ליתר לחץ דם עובדות טוב יותר כאשר נוטלים אותן באופן קבוע כמושחם. לעולם אל תדלגו על מנות ולעולם אל תפסיקו לקחת את התרופות שלכם ללא התייעצות עם הרופא שלכם.";

"health_tip_bp_calcium_title" = "הגבירו את צריכת הסידן";
"health_tip_bp_calcium_description" = "צריכת סידן מספקת מסייעת בויסות לחץ דם. כללו מוצרי חלב דלי שומן, משקאות צמחיים מעושרים או תוספי סידן אם מומלץ על ידי הרופא שלכם.";

"health_tip_bp_smoking_title" = "הפסיקו לעשן";
"health_tip_bp_smoking_description" = "כל סיגריה מעלה את לחץ הדם באופן זמני. הפסקת עישון מסייעת להפחית את לחץ הדם ומשפרת את בריאות הלב וכלי הדם באופן כללי.";

"health_tip_bp_breathing_title" = "תרגלו נשימה עמוקה";
"health_tip_bp_breathing_description" = "נשימה איטית ועמוקה במשך 5-10 דקות ביום יכולה לסייע בהפחתת לחץ דם. נסו לנשום עמוק דרך האף ולנשוף דרך הפה.";

"health_tip_bp_processed_title" = "הגבילו מזונות מעובדים";
"health_tip_bp_processed_description" = "מזונות מעובדים מאוד מכילים לעתים קרובות נתרן מוסתר ושומנים לא בריאים. בחרו במזונות שלמים ולא מעובדים ככל האפשר.";

"health_tip_bp_sleep_title" = "שמרו על שינה סדירה";
"health_tip_bp_sleep_description" = "שאפו ל-7-8 שעות שינה איכותית כל לילה. איכות שינה ירודה ודום נשימה בשינה עלולים לתרום ליתר לחץ דם.";

/* Weight Management Tips */
"health_tip_weight_portion_title" = "תרגלו בקרת מנות";
"health_tip_weight_portion_description" = "השתמשו בצלחות קטנות יותר, מדדו מנות והיו מודעים לגודל המנות. אפילו מזונות בריאים יכולים לתרום לעלייה במשקל כאשר המנות גדולות מדי.";

"health_tip_weight_mindful_title" = "אכלו במודעות";
"health_tip_weight_mindful_description" = "האטו בזמן האכילה, תיהנו מכל ביס ושימו לב לסימני רעב ושובע. הימנעו מהסחות דעת כמו טלוויזיה או טלפונים במהלך הארוחות.";

"health_tip_weight_protein_title" = "הגבירו את צריכת החלבון";
"health_tip_weight_protein_description" = "חלבון מסייע לתחושת שובע ושמירה על מסת שריר. כללו מקורות חלבון רזים בכל ארוחה, כמו דגים, עוף רזה, ביצים, טופו או קטניות.";

"health_tip_weight_fiber_title" = "בחרו מזונות עשירים בסיבים";
"health_tip_weight_fiber_description" = "סיבים תזונתיים מסייעים לתחושת שובע ומשפרים את בריאות העיכול. הגבירו את צריכת הפירות, הירקות, הקטניות והדגנים המלאים.";

"health_tip_weight_hydration_title" = "שתו מים לפני הארוחות";
"health_tip_weight_hydration_description" = "שתיית כוס מים לפני הארוחה יכולה לסייע בהפחתת התיאבון. לעתים קרובות אנו מבלבלים בין צמא לרעב.";

