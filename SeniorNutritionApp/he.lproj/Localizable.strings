"health_tip_diabetes_meal_timing_title" = "אכלו את הארוחות שלכם בשעות קבועות";
"health_tip_diabetes_meal_timing_description" = "נסו לאכול באותה שעה בכל יום כדי לשמור על רמת סוכר יציבה בדם. הימנעו מלדלג על ארוחות, במיוחד אם אתם נוטלים תרופות.";

"health_tip_diabetes_active_title" = "הישארו פעילים לאחר הארוחות";
"health_tip_diabetes_active_description" = "הליכה קצרה לאחר ארוחות יכולה לעזור להפחית את עליות הסוכר לאחר הארוחה. שאפו להליכה של 10-15 דקות לאחר הארוחה העיקרית שלכם ביום.";

"health_tip_diabetes_stress_title" = "נהלו את רמות המתח שלכם";
"health_tip_diabetes_stress_description" = "מתח יכול להעלות את רמת הסוכר בדם. תרגלו טכניקות הרגעה כמו נשימה עמוקה, מדיטציה או יוגה עדינה כדי לעזור בניהול המתח.";

"health_tip_diabetes_feet_title" = "בדקו את הרגליים שלכם מדי יום";
"health_tip_diabetes_feet_description" = "אנשים עם סוכרת צריכים לבדוק את הרגליים שלהם מדי יום לחיתוכים, שלפוחיות, אדמומיות או נפיחות. טיפול טוב ברגליים מונע סיבוכים.";

"health_tip_diabetes_hydration_title" = "הישארו מיוהדרים";
"health_tip_diabetes_hydration_description" = "הידרציה טובה מסייעת לכליות שלכם לסלק עודף סוכר דרך השתן. שאפו ל-8 כוסות מים ביום, יותר אם רמת הסוכר שלכם גבוהה.";

"health_tip_diabetes_fiber_title" = "הכלילו סיבים בכל ארוחה";
"health_tip_diabetes_fiber_description" = "סיבים מאטים את ספיגת הסוכר ומשפרים את רמת הסוכר בדם. שאפו ל-25-30 גרם סיבים יומיים מירקות, פירות, דגנים מלאים וקטניות.";

"health_tip_diabetes_alcohol_title" = "הגבילו צריכת אלכוהול";
"health_tip_diabetes_alcohol_description" = "אלכוהול עלול להוריד את רמת הסוכר בדם באופן מסוכן אם אתם נוטלים אינסולין או תרופות מסוימות לסוכרת. אם אתם שותים, עשו זאת במתינות ותמיד עם אוכל.";

"health_tip_diabetes_abc_title" = "הכירו את ה-ABC שלכם";
"health_tip_diabetes_abc_description" = "לניהול סוכרת, זכרו את ה-ABC שלכם: A1C (בדיקת סוכר), לחץ דם (Blood pressure) וכולסטרול (Cholesterol). שמרו על שלושתם בטווחים המטרה.";

"health_tip_diabetes_portion_title" = "בקרת מנות חשובה";
"health_tip_diabetes_portion_description" = "אפילו מזונות בריאים יכולים להעלות את רמת הסוכר בדם אם המנות גדולות מדי. השתמשו בכוסות מדידה או משקל מזון עד שתוכלו להעריך במדויק גדלי מנות.";

"health_tip_diabetes_fats_title" = "בחרו שומנים בריאים";
"health_tip_diabetes_fats_description" = "החליפו שומנים רוויים בשומנים בריאים ממקורות כמו שמן זית, אבוקדו ואגוזים כדי לשפר את רגישות האינסולין ובריאות הלב.";

"health_tip_diabetes_low_sugar_title" = "היו מוכנים להיפוגליקמיה";
"health_tip_diabetes_low_sugar_description" = "החזיקו תמיד פחמימה מהירת פעולה כמו טבליות גלוקוז, מיץ או סוכריות קשות לטיפול בהיפוגליקמיה (רמת סוכר נמוכה בדם).";

"health_tip_diabetes_eye_title" = "תכננו בדיקות עיניים סדירות";
"health_tip_diabetes_eye_description" = "סוכרת מגדילה את הסיכון לבעיות עיניים. ערכו בדיקת עיניים מקיפה עם הרחבת אישונים לפחות פעם בשנה כדי לגלות בעיות מוקדם.";

"health_tip_diabetes_sleep_title" = "תנו עדיפות לשינה איכותית";
"health_tip_diabetes_sleep_description" = "שינה לקויה יכולה להשפיע על שליטה ברמת הסוכר בדם ורגישות לאינסולין. שאפו ל-7-8 שעות שינה איכותית כל לילה.";

/* Blood Pressure Tips */
