// Health Tip Descriptions
"health_tip_sleep_description" = "שאפו ל-7-8 שעות שינה איכותית כל לילה. לוח זמני שינה עקבי עוזר לווסת את חילוף החומרים ומקדם בריאות כללית.";
"health_tip_stress_description" = "תרגלו טכניקות הרפיה כמו נשימות עמוקות, מדיטציה או יוגה עדינה לניהול לחץ, שיכול להשפיע על בריאות הלב והעיכול.";
"health_tip_checkups_description" = "בדיקות בריאות קבועות הן חיוניות. תכננו ביקורים אצל הרופא לפחות פעם בשנה, יותר אם יש לכם מצבים כרוניים.";

"health_tip_protein_description" = "מבוגרים זקוקים ליותר חלבון לשמירה על מסת השריר. שאפו ל-1-1.2 גרם לק\"ג משקל גוף יומי ממקורות כמו בשר רזל, דגים, ביצים, מוצרי חלב וחלבונים צמחיים.";
"health_tip_fiber_description" = "אכלו מזונות עשירים בסיבים כמו דגנים מלאים, פירות, ירקות וקטניות לתמיכה בבריאות העיכול וניהול רמות הכולסטרול.";
"health_tip_calcium_description" = "מבוגרים מעל גיל 50 זקוקים ל-1200 מ\"ג סידן יומי. כללו מוצרי חלב, משקאות צמחיים מועשרים, דגים משומרים עם עצמות וירקות עליים בתזונה שלכם.";
"health_tip_vitaminD_description" = "ויטמין D עוזר בספיגת סידן. קבלו קצת חשיפה לשמש ושקלו תוסף, במיוחד בחודשי החורף.";

"health_tip_fasting_hydration_description" = "שתו הרבה מים בזמן תקופות הצום שלכם. הישארו מיוהדרים עם לפחות 8-10 כוסות ביום למניעת התייבשות.";
"health_tip_fasting_listen_body_description" = "אם אתם מרגישים לא טוב בזמן צום - חווים סחרחורת, עייפות קיצונית או בלבול - הפסיקו את הצום מיד.";
"health_tip_fasting_break_description" = "שברו את הצום עם מזונות קלים לעיכול כמו מרקים, פירות או מנות קטנות של חלבון למניעת אי נוחות בעיכול.";
"health_tip_fasting_sleep_description" = "שינה איכותית עוזרת לתוצאות הצום. שמרו על לוח זמני שינה עקבי לתמיכה בשגרת הצום לסירוגין שלכם.";

"health_tip_hydration_reminder_description" = "הגדירו תזכורות לשתיית מים לאורך היום. תחושת הצמא שלכם עשויה לפחות עם הגיל, מה שהופך תזכורות מתוזמנות למועילות.";
"health_tip_hydration_foods_description" = "פירות כמו אבטיח, תפוזים, וירקות כמו מלפפון וחסה הם מקורות מצוינים להידרציה מעבר למים רגילים.";

"health_tip_activity_walking_description" = "הליכה יומית של 30 דקות משפרת את הזרימה, בריאות הלב ומצב הרוח. זהו אחד התרגילים הפשוטים והיעילים ביותר למבוגרים.";
"health_tip_activity_strength_description" = "אפילו אימון כוח קל 2-3 פעמים בשבוע עוזר לשמור על מסת שריר, צפיפות עצם ועצמאות עם ההזדקנות.";

"health_tip_medication_schedule_description" = "קחו תרופות באותה שעה כל יום לשמירה על רמות קבועות בזרם הדם ולמקסום היעילות.";
"health_tip_medication_food_description" = "חלק מהתרופות צריכות להילקח עם אוכל. עקבו אחר הוראות הרופא שלכם לגבי תזמון התרופות עם הארוחות.";

"health_tip_senior_balance_description" = "תרגלו תרגילי שיווי משקל כמו עמידה על רגל אחת או הליכת עקב-בהונות למניעת נפילות, שהן סיכון משמעותי לקשישים.";
"health_tip_senior_vision_description" = "בדקו את העיניים מדי שנה. ראייה טובה חיונית למניעת נפילות ולשמירה על איכות החיים.";
"health_tip_senior_social_description" = "אינטראקציה חברתית קבועה יכולה לסייע בשמירה על תפקוד קוגניטיבי ורווחה רגשית. שקלו להצטרף לקבוצות קהילתיות, לקבוע שיחות וידאו קבועות עם המשפחה או להשתתף בפעילויות מקומיות לגיל השלישי.";
"health_tip_senior_home_description" = "הפכו את ביתכם לבטוח יותר על ידי הבטחת תאורה טובה, הסרת מכשולים, התקנת מאחזי יד בחדרי האמבטיה ושמירת פריטים בשימוש תכוף בהישג יד. סביבה בטוחה מסייעת בשמירה על עצמאות.";
"health_tip_senior_memory_description" = "עסקו בתרגילים מנטליים יומיים כמו פאזלים, קריאה, למידת מיומנויות חדשות או משחקי זיכרון. גירוי מנטלי קבוע מסייע בשמירה על תפקוד קוגניטיבי ועשוי להפחית את הסיכון לבעיות זיכרון.";
"health_tip_senior_temperature_description" = "ככל שאנו מזדקנים, גופנו נעשה רגיש יותר לטמפרטורות קיצוניות. הישארו חמים במזג אוויר קר, הימנעו מחימום יתר בקיץ ושמרו על טמפרטורות נוחות בבית. זכרו להתלבש בשכבות ולהישאר מיודרים.";

