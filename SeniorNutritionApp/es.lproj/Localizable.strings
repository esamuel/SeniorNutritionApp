/* Health Tips - General */
"health_tip_sleep_title" = "Priorizar el Sueño de Calidad";
"health_tip_sleep_description" = "Intente dormir de 7 a 9 horas por noche. Un buen sueño es crucial para la recuperación, el equilibrio hormonal y la salud en general.";
"health_tip_stress_title" = "Manejar el Estrés";
"health_tip_stress_description" = "Practique técnicas de relajación como la meditación o la respiración profunda. El estrés crónico puede afectar negativamente su salud y sus hormonas.";
"health_tip_checkups_title" = "Chequeos Regulares";
"health_tip_checkups_description" = "Programe visitas regulares con su proveedor de atención médica para monitorear su salud y detectar cualquier problema a tiempo.";

/* Health Tips - Nutrition */
"health_tip_protein_title" = "Consumir Suficiente Proteína";
"health_tip_protein_description" = "La proteína es vital para mantener la masa muscular, especialmente en los adultos mayores. Incluya fuentes como carnes magras, pescado, huevos y legumbres.";
"health_tip_fiber_title" = "Enfóquese en la Fibra";
"health_tip_fiber_description" = "Los alimentos ricos en fibra como frutas, verduras y granos enteros ayudan a la digestión y pueden mejorar el control del azúcar en la sangre.";
"health_tip_calcium_title" = "Calcio para Huesos Fuertes";
"health_tip_calcium_description" = "Asegúrese de obtener suficiente calcio de productos lácteos, verduras de hoja verde o alimentos fortificados para mantener la salud ósea.";
"health_tip_vitaminD_title" = "No se Olvide de la Vitamina D";
"health_tip_vitaminD_description" = "La vitamina D ayuda a la absorción de calcio. Obténgala de la luz solar, alimentos fortificados o suplementos según lo recomiende su médico.";

/* Health Tips - Hydration */
"health_tip_hydration_reminder_title" = "Establecer Recordatorios de Hidratación";
"health_tip_hydration_reminder_description" = "Use la aplicación para configurar recordatorios para beber agua durante todo el día, no solo cuando tenga sed.";
"health_tip_hydration_foods_title" = "Comer Alimentos Hidratantes";
"health_tip_hydration_foods_description" = "Complemente su ingesta de líquidos con alimentos ricos en agua como pepinos, sandías y naranjas.";

/* Health Tips - Activity */
"health_tip_activity_walking_title" = "Caminar Diariamente";
"health_tip_activity_walking_description" = "Intente caminar al menos 30 minutos la mayoría de los días. Es una excelente manera de mantenerse activo y mejorar la salud cardiovascular.";
"health_tip_activity_strength_title" = "Entrenamiento de Fuerza";
"health_tip_activity_strength_description" = "Incorpore ejercicios de entrenamiento de fuerza 2-3 veces por semana para mantener la masa muscular y el metabolismo.";

/* Health Tips - Medication */
"health_tip_medication_schedule_title" = "Siga su Horario de Medicamentos";
"health_tip_medication_schedule_description" = "Tome sus medicamentos según lo prescrito. Use los recordatorios de la aplicación para no olvidar una dosis.";
"health_tip_medication_food_title" = "Tome los Medicamentos con Comida si es Necesario";
"health_tip_medication_food_description" = "Algunos medicamentos deben tomarse con alimentos para una mejor absorción o para evitar efectos secundarios. Consulte a su farmacéutico.";

/* Health Tips - Senior-Specific */
"health_tip_senior_balance_title" = "Mejorar el Equilibrio";
"health_tip_senior_balance_description" = "Practique ejercicios de equilibrio para reducir el riesgo de caídas. Actividades como el tai chi o el yoga pueden ser muy beneficiosas.";
"health_tip_senior_vision_title" = "Cuidado de la Vista";
"health_tip_senior_vision_description" = "Hágase exámenes oculares regulares. Una buena visión es crucial para la seguridad y para disfrutar de las actividades diarias.";

/* Health Tips - Fasting */
"health_tip_fasting_hydration_title" = "Hidratación durante el ayuno";
"health_tip_fasting_hydration_description" = "Considere beber mucha agua durante sus períodos de ayuno. Intente tomar al menos 8-10 vasos diarios para ayudar a prevenir la deshidratación.";

"health_tip_fasting_listen_body_title" = "Escuche a su cuerpo";
"health_tip_fasting_listen_body_description" = "Si no se siente bien durante el ayuno - como mareos, fatiga extrema o confusión - considere romper su ayuno y consulte con su proveedor de salud.";

"health_tip_fasting_break_title" = "Romper el ayuno";
"health_tip_fasting_break_description" = "Podría encontrar útil romper su ayuno con alimentos fácilmente digeribles como sopas, frutas o pequeñas porciones de proteína para evitar molestias digestivas.";

"health_tip_fasting_sleep_title" = "Sueño y ayuno";
"health_tip_fasting_sleep_description" = "El sueño de calidad puede ayudar a mantener su rutina de ayuno. Considere mantener un horario de sueño regular para facilitar sus objetivos de ayuno intermitente.";

/* Health Tips - Hydration */
"health_tip_hydration_reminder_title" = "Recordatorios de hidratación";
"health_tip_hydration_reminder_description" = "Podría encontrar útil configurar recordatorios para beber agua durante todo el día, ya que la sensación de sed puede disminuir con la edad.";

"health_tip_hydration_foods_title" = "Alimentos hidratantes";
"health_tip_hydration_foods_description" = "Considere incluir alimentos hidratantes como sandía, naranjas, pepino y lechuga en su dieta como fuentes adicionales de hidratación además del agua.";

/* Health Tips - Activity */
"health_tip_activity_walking_title" = "Caminata diaria";
"health_tip_activity_walking_description" = "Una caminata diaria de 30 minutos puede ayudar a mejorar la circulación, la salud cardíaca y el estado de ánimo. Muchas personas mayores encuentran que es una de las formas de ejercicio más accesibles y efectivas.";

"health_tip_activity_strength_title" = "Ejercicios de fuerza";
"health_tip_activity_strength_description" = "Considere un entrenamiento de fuerza ligero 2-3 veces por semana, ya que muchas personas mayores encuentran que ayuda a mantener la masa muscular, la densidad ósea y la independencia.";

/* Health Tips - Medication */
"health_tip_medication_schedule_title" = "Horario de medicamentos";
"health_tip_medication_schedule_description" = "Tomar medicamentos a horarios consistentes cada día puede ayudar a mantener niveles estables en su torrente sanguíneo. Considere discutir el mejor horario con su proveedor de salud.";

"health_tip_medication_food_title" = "Medicamentos y comidas";
"health_tip_medication_food_description" = "Algunos medicamentos necesitan ser tomados con alimentos. Considere discutir con su proveedor de salud sobre el mejor momento para tomar sus medicamentos en relación con las comidas.";

/* Health Tips - Senior Specific */
"health_tip_senior_balance_title" = "Ejercicios de equilibrio";
"health_tip_senior_balance_description" = "Practique ejercicios de equilibrio como pararse en un pie o caminar talón-punta para prevenir caídas, que son un riesgo importante para los adultos mayores.";

"health_tip_senior_vision_title" = "Salud visual";
"health_tip_senior_vision_description" = "Hágase revisar los ojos anualmente. Una buena visión es crucial para prevenir caídas y mantener la calidad de vida.";

